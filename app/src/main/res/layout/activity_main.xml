        android:id="@+id/map"
        tools:context="com.io.luis_santiago.googlemaps.MainActivity"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        />

    <include layout="@layout/menu_layout"/>
</RelativeLayout>
